+		ethernet0 = &gem0;
+		serial0 = &uart0;
+		serial1 = &uart1;
+		spi0 = &spi0;
+		i2c0 = &i2c0;
+		i2c1 = &i2c1;
+		i2c2 = &tun;
+		i2c3 = &i2c_aux2;
+		i2c4 = &i2c_aux1;
+		i2c5 = &i2c_daq0;
+		i2c6 = &i2c_daq1;
+		i2c7 = &i2c_daq2;
+		i2c8 = &i2c_nfc0;
+		i2c9 = &i2c_nfc1;
+	};
+
+	memory {
+		device_type = "memory";
+		reg = <0x0 0x40000000>;
+	};
+
